{
  "name": "@jonasdoesthings/github-contributions",
  "version": "1.0.0",
  "description": "simple library for fetching a user's github contribution graph",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/JonasDoesThings/github-contributions",
